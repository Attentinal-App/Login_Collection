<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 버튼 배경 색상 설정 -->
    <solid android:color="#FFFFFF" />

    <!-- 테두리 설정 -->
    <stroke
        android:width="2dp"
        android:color="#6200EE" /> <!-- 보라색 테두리 -->

    <!-- 사각형 모양 설정 (radius는 0으로) -->
    <corners android:radius="0dp" />
</shape>
